{
  "name": "seq-boilerplate",
  "version": "0.0.2",
  "private": true,
  "description": "sequelize boilerplate",
  "scripts": {
    "start": "node ./bin/www",
    "prod": "NODE_ENV=production npm start",
    "dev": "NODE_ENV=development DEBUG=whatsapp:*,sequelize-cacher:* npm start",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "mocha test/unit/*.test.js",
    "test-integration": "mocha test/integration/*.test.js"
  },
  "keywords": [
    "sequelize",
    "boilerplate",
    "express"
  ],
  "dependencies": {
    "body-parser": "~1.13.2",
    "chalk": "1.1.3",
    "config": "^1.21.0",
    "consolidate": "^0.14.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "figlet": "1.1.2",
    "handlebars": "^4.0.5",
    "memcached": "^2.2.2",
    "morgan-debug": "^1.0.0",
    "mysql": "^2.11.1",
    "sequelize": "^3.3.2",
    "sequelize-cacher": "^1.3.2",
    "sequelize-cli": "2.4.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8",
    "sqlite3": "^3.0.8"
  },
  "devDependencies": {
    "bluebird": "^2.9.33",
    "expect.js": "^0.3.1",
    "mocha": "^2.2.5",
    "supertest": "^1.0.1"
  }
}
